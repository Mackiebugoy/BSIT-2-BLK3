import 'package:flutter/material.dart';


class labExam extends StatelessWidget {
  @override
  Widget build(BuildContext context) {
    return MaterialApp(
      home: LabExamHome(),
    );
  }
}


class LabExamHome extends StatelessWidget {
  @override
  Widget build(BuildContext context) {
    return Scaffold(
      appBar: AppBar(
        title: Text.('Lab Examination'),
      ),
      body: Center(
        child: Column(
          mainAxisAlignment: MainAxisAlignment.center,
          children: [
            // Use ClipOval to make the image round
            ClipOval(
              child: Image.network(
                'https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQqGK3diR3Zi-mnOXEaj-3ewmFyRYVxGzVzZw&s', // Replace with your image URL
                width: 100, // Set the width of the round image
                height: 100, // Set the height of the round image
                fit: BoxFit.cover, // Adjust the fit
              ),
            ),
            SizedBox(height: 20), // Add some space between the image and text
            Text('Welcome to Lab Examination'),
          ],
        ),
      ),
    );
  }
}
