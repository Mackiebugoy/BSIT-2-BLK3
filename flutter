import 'package:flutter/material.dart';


class BasicWidget extends StatelessWidget {
  @override
  Widget build(BuildContext context) {
    return MaterialApp(
      home: BasicWidgetsHome(),
      theme: ThemeData(
        useMaterial3: true,

        // Define the default brightness and colors.
        colorScheme: ColorScheme.fromSeed(
          seedColor: Colors.blueGrey,
          brightness: Brightness.dark,
        ),
      ),
    ); // Closing this MaterialApp widget
  }
}


class BasicWidgetsHome extends StatelessWidget {
  @override
  Widget build(BuildContext context) {
    return Scaffold(
      appBar: AppBar(
        title: Text('Basic Widgets Home'),

      ),
      body: Center(
        child: Column(
          children: [
            Image.network('data:image/jpeg;base64,'),
            Image.network('images/mac.jpg')
           ],
        ),
      ),
    );
  }
}
